<!-- Header
================================================== -->

<!-- 960 Container -->
<div class="container ie-dropdown-fix">

	<!-- Header -->
	<div id="header">

		<!-- Logo -->
		<div class="eight columns">
			<div id="logo">
				<a href="~site_url~/index"><img src="~theme_dir~/images/logo.png" alt="logo" /></a>
				<div id="tagline">Welcome to ~domain_name~!</div>
				<div class="clear"></div>
			</div>
		</div>

		<!-- Social / Contact -->
		<div class="eight columns">
			
			<!-- Social Icons -->
			<ul class="social-icons">
				<li class="facebook"><a href="http://facebook.com/">Facebook</a></li>
				<li class="twitter"><a href="http://twitter.com/">Twitter</a></li>
				<li class="dribbble"><a href="http://dribble.com/">Dribbble</a></li>
				<li class="linkedin"><a href="http://linkedin.com/">LinkedIn</a></li>
				<li class="pintrest"><a href="http://pinterest.com/">Pintrest</a></li>
			</ul>
			
			<div class="clear"></div>
			
			<!-- Contact Details -->
			<div id="contact-details">
				<ul>
					<li><i class="mini-ico-envelope"></i><a href="mailto:~support_cservice_email~">~support_cservice_email~</a></li>
					<li><i class="mini-ico-user"></i>~support_cservice_phone~</li>
				</ul>
			</div>

		</div>

	</div>
	<!-- Header / End -->
	
	<!-- Navigation -->
	<div class="sixteen columns">

		<div id="navigation">
				<e:nav_menu>
			


		</div> 
		<div class="clear"></div>
		
	</div>
	<!-- Navigation / End -->

</div>
<!-- 960 Container / End -->


